{
  "name": "vite-vue-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "npm run dev:development",
    "dev": "npm run dev:development",
    "dev:development": "vite --mode development",
    "dev:pre": "vite --mode pre",
    "dev:release": "vite --mode release",
    "dev:production": "vite --mode production",
    "build": "npm run build:development",
    "build:development": "vue-tsc --noEmit --skipLibCheck && vite build --mode development",
    "build:pre": "vue-tsc --noEmit --skipLibCheck && vite build --mode pre",
    "build:release": "vue-tsc --noEmit --skipLibCheck && vite build --mode release",
    "build:production": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
    "create:route": "node scripts/create-route/index.js",
    "preview": "npm run build && vite preview",
    "prepare": "husky install",
    "lint": "vue-tsc --noEmit --skipLibCheck && eslint \"src/**/*.{js,ts,jsx,vue}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,jsx,vue}\"",
    "commit": "vue-tsc --noEmit --skipLibCheck && git add . && git cz",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@iconify/iconify": "^2.2.1",
    "@syyfe/vite-plugin-cos": "0.0.4",
    "@vueuse/core": "^8.7.5",
    "@wangeditor/editor": "^5.1.0",
    "@wangeditor/editor-for-vue": "^5.1.11",
    "axios": "^0.26.1",
    "cos-js-sdk-v5": "^1.3.10",
    "custom-vue-scrollbar": "^0.0.7",
    "dayjs": "^1.11.1",
    "echarts": "^5.3.2",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.13",
    "qrcode": "^1.5.0",
    "qs": "^6.10.3",
    "screenfull": "^6.0.1",
    "swiper": "^8.3.0",
    "vue": "^3.2.25",
    "vue-awesome-swiper": "^5.0.1",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.0.14",
    "vue3-draggable-resizable": "^1.6.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.36.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify/json": "^2.1.29",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^13.1.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.50.1",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.8.10",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.5.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
